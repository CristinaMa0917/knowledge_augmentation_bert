rm text_bert_pretrain.tar.gz
tar -czf text_bert_pretrain.tar.gz ./configs ./custom_estimator ./data_dumper ./data_loader ./main ./model ./trainer ./util ./resources

CHECKPOINT_PATH=text_bert_pretrain/finetune/unify_dense_before_cate_53w_short_query
BERT_PATH=text_bert_pretrain/pretrain/minibert_pretrain_unify_dense_before_cate
